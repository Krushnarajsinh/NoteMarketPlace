    <!-- <form class="add-notes-form space-remove" >-->
    @using (Html.BeginForm("AddNotes", "SellYourNotes", FormMethod.Post, new { enctype = "multipart/form-data", @class = "add-notes-form space-remove" }))
    {

        @Html.AntiForgeryToken()
        <!-- First Section -->
        <section id="personal-details" class="user-profile content-background-md">
            <div>
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-12">
                            <div class="heading">
                                <h3> Basic Note Details</h3>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-12">
                            <div class="row">

                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <!-- Title -->
                                    <div class="form-group">
                                        <label class="control-label"> Title<span class="required"> *</span> </label>
                                        @Html.TextBoxFor(model => model.Title, new { @class = "form-control", id = "fname", placeholder = "Enter your notes title", required = "required" })
                                        @Html.ValidationMessageFor(model => model.Title, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                        <!-- <input type="text" class="form-control" id="fname" placeholder="Enter your notes title" > -->
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <!-- Category -->
                                    <div class="form-group">
                                        <label class=" control-label" for="gender"> Category<span class="required"> *</span> </label>
                                        <!-- <select class="form-control inputselect" >
                                        <option > Select your category</option >
                                        </select > -->
                                        @Html.DropDownListFor(model => model.Category, new SelectList(Model.NoteCategoryList, "ID", "Name", Model.Category), "Select your notes Category", new { @class = "form-control inputselect", id = "category", required = "required" })
                                        @Html.ValidationMessageFor(model => model.Category, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <!-- Display Picture -->
                                    <div class="form-group">
                                        <div class="upload-pic">
                                            <label class="control-label" for="InputEmail1"> Display Picture</label>
                                            <!-- <input type="file" class="form-control InputEmail1" aria-describedby="emailHelp" > -->
                                            @Html.TextBoxFor(model => model.DisplayPicture, new { @class = "form-control InputEmail1", type = "file", accept = ".jpg , .jpeg , .png" })
                                            @Html.ValidationMessageFor(model => model.DisplayPicture, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                            <div class="mypic text-center">
                                                <img class="img-fluid" src="~/Img/Front/user-profile/upload.png">
                                                <h6> Upload a picture</h6>
                                            </div>

                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <!-- Upload Notes -->
                                    <div class="form-group">
                                        <div class="upload-pic">
                                            <label class="control-label" for="InputEmail1"> Upload Notes<span class="required"> *</span> </label>
                                            <!-- <input type="file" class="form-control InputEmail1" aria-describedby="emailHelp" > -->
                                            @Html.TextBoxFor(model => model.UploadNotes, new { @class = "form-control InputEmail1", id = "upload-notes", type = "file", required = "required", accept = ".pdf", multiple = "multiple" })
                                            @Html.ValidationMessageFor(model => model.UploadNotes, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                            <div class="mypic text-center">
                                                <img class="img-fluid" src="~/Img/Front/user-profile/upload.png">
                                                <h6> Upload your notes</h6>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>


                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <!-- Type -->
                                    <div class="form-group">
                                        <label class=" control-label" for="gender"> Type</label>
                                        @Html.DropDownListFor(model => model.NoteType, new SelectList(Model.NoteTypeList, "ID", "Name", Model.NoteType), "Select your notes type", new { @class = "form-control inputselect", id = "type" })
                                        @Html.ValidationMessageFor(model => model.NoteType, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                        <!-- <select class="form-control inputselect" >
                                        <option > Select your note type</option >
                                        <option > Type 1</option >
                                        <option > Type 2</option >
                                        <option > Type 3</option >
                                        </select > -->
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <!-- Number of Pages -->
                                    <div class="form-group">
                                        <label class="control-label"> Number of Pages</label>
                                        <!-- <input type="text" class="form-control" id="fname" placeholder="Enter number of note pages" > -->
                                        @Html.TextBoxFor(model => model.NumberofPages, new { @class = "form-control", id = "fname", placeholder = "Enter number of note pages" })
                                        @Html.ValidationMessageFor(model => model.NumberofPages, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                    </div>
                                </div>
                            </div>
                            <!-- Description -->
                            <div class="row">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <div class="form-group">
                                        <div class="textarea-input dashboard ">
                                            <label class="control-label"> Description<span class="required"> *</span> </label>
                                            <!-- <textarea placeholder="Enter your description " class="form-control InputEmail1" id="text-ar" > </textarea > -->
                                            @Html.TextAreaFor(model => model.Description, new { @class = "form-control InputEmail1", id = "text-ar", placeholder = "Enter your description", required = "required" })
                                            @Html.ValidationMessageFor(model => model.Description, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </section>
        <!-- Basic Detail Ends-- >
        <!-- Instututional Detail -->
        <section id="study-details" class="user-profile content-background-md">
            <div>
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 col-sm-12">
                            <div class="heading">
                                <h3> Istitution Information</h3>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12 col-sm-12">
                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <!-- University -->
                                    <div class="form-group">
                                        <label class=" control-label" for="gender"> Country</label>
                                        <!-- <select class="form-control inputselect" >
                                        <option > Select your country</option >
                                        <option > India</option >
                                        </select > -->
                                        @Html.DropDownListFor(model => model.Country, new SelectList(Model.CountryList, "ID", "Name", Model.Country), "Select country", new { @class = "form-control inputselect", id = "country" })
                                        @Html.ValidationMessageFor(model => model.Country, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <!-- College -->
                                    <div class="form-group">
                                        <label class="control-label" for="InputEmail1"> Institution Name</label>
                                        <!-- <input type="text" class="form-control InputEmail1" aria-describedby="emailHelp" placeholder="Enter your institution name" > -->
                                        @Html.TextBoxFor(model => model.UniversityName, new { @class = "form-control InputEmail1", id = "institution-name", placeholder = "Enter your institution name" })
                                        @Html.ValidationMessageFor(model => model.UniversityName, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                    </div>

                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </section>
        <!-- Instututional Detail Ends-- >
        <!-- Course Detail -->
        <section id="Address-details" class="user-profile content-background-md">
            <div>
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 col-sm-12  col-12">
                            <div class="heading">
                                <h3> Course Details</h3>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-12">

                            <div class="row">

                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <!-- Address Line 1 -->
                                    <div class="form-group">
                                        <label class="control-label"> Course Name</label>
                                        <!-- <input type="text" class="form-control InputEmail1" placeholder="Enter your course name" > -->
                                        @Html.TextBoxFor(model => model.Course, new { @class = "form-control InputEmail1", id = "course-name", placeholder = "Enter your course name" })
                                        @Html.ValidationMessageFor(model => model.Course, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                    </div>

                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <!-- Address Line 2 -->
                                    <div class="form-group">
                                        <label class="control-label" for="InputEmail1"> Course Code</label>
                                        <!-- <input type="text" class="form-control InputEmail1" aria-describedby="emailHelp" placeholder="Enter your course code" > -->
                                        @Html.TextBoxFor(model => model.CourseCode, new { @class = "form-control InputEmail1", id = "course-code", placeholder = "Enter your course code" })
                                        @Html.ValidationMessageFor(model => model.CourseCode, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                    </div>

                                </div>

                            </div>
                            <div class="row">

                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <!-- City -->
                                    <div class="form-group">
                                        <label class="control-label" for="InputEmail1"> Professor / Lecturer</label>
                                        <!-- <input type="email" class="form-control InputEmail1" aria-describedby="emailHelp" placeholder="Enter your professor name" > -->
                                        @Html.TextBoxFor(model => model.Professor, new { @class = "form-control InputEmail1", id = "proffesor", placeholder = "Enter your proffesor name" })
                                        @Html.ValidationMessageFor(model => model.Professor, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </section>
        <!-- Course Detail -->
        <!-- Selling type Detail -->
        <section id="sell-details" class="user-profile content-background-md">
            <div>
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 col-sm-12">
                            <div class="heading">
                                <h3> Selling Information</h3>
                            </div>
                        </div>
                    </div>
                    <div class="row" id="add-form">
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <!-- University -->
                            <div class="form-group">
                                <label class="control-label"> Sell For *</label>
                                <div class="row giveinnerpadding">
                                    <div class="col-md-6 col-sm-6 col-6  col-xs-6 ">
                                        <!-- <input name="sell" type="radio" class=" InputEmail1" > -->
                                        @Html.RadioButtonFor(model => model.IsPaid, "false", new { @class = "form-check-input select-sell-mode InputEmail1", id = "free" })
                                        <label class="control-label"> Free</label>

                                    </div>
                                    <div class="col-md-6 col-sm-6  col-6  col-xs-6 paid ">
                                        <!-- <input name="sell" type="radio" class=" InputEmail1" > -->
                                        @Html.RadioButtonFor(model => model.IsPaid, "true", new { @class = "form-check-input select-sell-mode InputEmail1", id = "paid" })
                                        <label class="control-label "> Paid</label>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label" for="InputEmail1"> Sell Price<span class="required"> *</span> </label>
                                <!-- <input type="text" class="form-control InputEmail1" aria-describedby="emailHelp" placeholder="Enter your institution name" > -->
                                @Html.TextBoxFor(model => model.SellingPrice, new { @class = "form-control InputEmail1", id = "price", placeholder = "Enter your notes price", disabled = "disabled" })
                                @Html.ValidationMessageFor(model => model.SellingPrice, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                            </div>

                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <div class="form-group">
                                <div class="upload-pic set-pic">
                                    <label class="control-label" for="InputEmail1"> Note Preview</label>
                                    <!-- <input type="file" class="form-control InputEmail1" aria-describedby="emailHelp" > -->
                                    @Html.TextBoxFor(model => model.NotesPreview, new { @class = "form-control InputEmail1", id = "note-preview", type = "file", accept = ".pdf" })
                                    @Html.ValidationMessageFor(model => model.NotesPreview, "", new { @class = "form-error", style = "color:red;  padding: 0px 0px 0px 35px;" })
                                    <div class="mypic text-center mypic-set">
                                        <img class="img-fluid" src="~/Img/Front/user-profile/upload.png">
                                        <h6> Upload a file</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Push & Submit Button -->
                    <div class="row">
                        <div class="col-md-6 col-sm-6 col-6 col-sm-6 col-xs-12">
                            <div class="row">
                                <div class="col-md-6 col-sm-6 col-6 col-sm-6 col-xs-12">
                                    <button type="submit" class="btn btn-primary btn-block login-btn text-uppercase text-center left-btn" id="mybtn"> <b> submit</b> </button>
                                </div>
                                <div class="col-md-6 col-sm-6 col-6  col-xs12">
                                    <a role="button" class="btn btn-primary  btn-block login-btn text-uppercase text-center right-btn" id="mybtn" href="@Url.Action("PublishNote", new { id = @Model.ID })"><b>Publish</b></a>
                                    <!--  <button type="submit" class="btn btn-primary  btn-block login-btn text-uppercase text-center right-btn" id="mybtn" > <b > Publish</b > </button > -->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    }
    <!-- </form > -->
    <!-- Selling type Detail -->

    <!-- footer -->

    <footer>
        <div class="end">
            <div class="container">
                <div class="row">
                    <div class="col-md-6 col-12 col-sm-6 col-xs-8">
